\glossaryentry{v1.9>\approvalpage=\verb!*+\approvalpage+:>Tweaked the spacing on the title page: the term ``dissertation'' or ``thesis'' now starts on its own line.}{22}
\glossaryentry{v1.1>edication=\verb!*+dedication+:>Fixed PDF page numbering on dedication page}{24}
\glossaryentry{v1.6>edication=\verb!*+dedication+:>Added ``Dedication Page'' to PDF bookmarks if \texttt  {hyperref} has been loaded}{24}
\glossaryentry{v1.1>pigraph=\verb!*+epigraph+:>Fixed PDF page numbering on epigraph page}{24}
\glossaryentry{v1.6>pigraph=\verb!*+epigraph+:>Added ``Epigraph'' to the PDF bookmarks if \texttt  {hyperref} has been loaded.}{24}
\glossaryentry{v1.5>cknowledgments=\verb!*+acknowledgments+:>Added commands to switch the layout of even and odd pages and redefined things so that all frontmatter after Acknowledgments starts on even numbered pages that are actually printed as odd numbered pages in the PDF\spacefactor \@m {}.}{24}
\glossaryentry{v1.7>cknowledgments=\verb!*+acknowledgments+:>In the commands that flip the page margins in the front matter of two-sided documents, I changed \texttt  {\cs  {setlength}\{\cs  {a}\}\{\cs  {b}\}} to \texttt  {\cs  {a}\textequals \cs  {b}\cs  {relax}} so as to be more compatible with the \texttt  {calc} package. This was done because \texttt  {calc}'s changes to \cs  {setlength} broke my \cs  {reversepagemargins} and \cs  {restorepagemargins} commands.}{24}
\glossaryentry{v1.0>cknowledgments=\verb!*+acknowledgments+:>Fixed a bug with the last page of a 2+ page Acknowledgments section having no page number}{25}
\glossaryentry{v0.6>cknowledgments=\verb!*+acknowledgments+:>Added check to remind user to include acknowledgments if they are absent}{26}
\glossaryentry{v1.0>bstract=\verb!*+abstract+:>Tweaked font of title and spacing on Abstract page}{26}
\glossaryentry{v1.1>bstract=\verb!*+abstract+:>Added \cs  {phantomsection} and \cs  {texorpdfstring} to avoid conflicts with \texttt  {hyperref}}{26}
\glossaryentry{v1.2>bstract=\verb!*+abstract+:>Tweaked spacing on Abstract page again}{26}
\glossaryentry{v0.4>bstract=\verb!*+abstract+:>Make Abstract heading appear in ToC}{26}
\glossaryentry{v0.6>ita=\verb!*+vita+:>Added check to remind user to include vita if it is absent for a dissertation}{26}
\glossaryentry{v1.0>hebibliography=\verb!*+thebibliography+:>Modified \texttt  {thebibliography} to work with \texttt  {chapterbib} and \texttt  {natbib}}{27}
\glossaryentry{v0.2>hebibliography=\verb!*+thebibliography+:>Force \cs  {backmatter} at start of thebibliography}{27}
\glossaryentry{v1.6>hebibliography=\verb!*+thebibliography+:>Added per-chapter ``References'' sections to the table of contents if \texttt  {chapterbib} is used}{27}
\glossaryentry{v0.3>hebibliography=\verb!*+thebibliography+:>Fix bibliography entry spacing}{28}
\glossaryentry{v0.3> =General:>Made glossaries package work with this class}{29}
\glossaryentry{v1.1>\acronymname=\verb!*+\acronymname+:>Added \cs  {texorpdfstring} to ensure \texttt  {hyperref} compatibility.}{29}
\glossaryentry{v0.3> =General:>Made makeidx package work with this class}{30}
\glossaryentry{v1.1>\printindex=\verb!*+\printindex+:>Added \cs  {texorpdfstring} for \texttt  {hyperref} compatibility.}{30}
\glossaryentry{v1.8>\sixthreader=\verb!*+\sixthreader+:>Added definitions for ``hidden'' commands so they warn the user if the information is absent and provide more helpful information.}{30}
\glossaryentry{v0.6> =General:>Added environments for illustrations and schemes and made adjustments to be compatible with \texttt  {float} package}{31}
\glossaryentry{v1.8>\pdfbookmark=\verb!*+\pdfbookmark+:>Moved provided command for \cs  {pdfbookmark} (in case \texttt  {hyperref} is not loaded) to the end of the preamble.}{33}
\glossaryentry{v1.2> =General:>Moved loading of \texttt  {hyperref} to the end of the class, so that it's the last package loaded (fixing a bug with the appendix in the PDF outline).}{33}
